# /etc/profile: system-wide .profile file for the Bourne shells
echo "alpha Ready ..."
LD_LIBRARY_PATH=/lib:/usr/lib:$LD_LIBRARY_PATH
export LD_LIBRARY_PATH

USER="`id -un`"
LOGNAME=$USER
PS1='\[\033[1;34m\][\u@\h \w]\$\[\033[0m\] '
PATH=$PATH
HOSTNAME=`/bin/hostname`

export USER LOGNAME PS1 PATH


